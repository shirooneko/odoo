<odoo>
    <record id="view_publisher_form" model="ir.ui.view">
        <field name="name">library.publisher.form</field>
        <field name="model">library.publisher</field>
        <field name="arch" type="xml">
            <form string="Penerbit">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="address"/>
                        <field name="phone"/>
                        <field name="email"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_publisher_tree" model="ir.ui.view">
        <field name="name">library.publisher.tree</field>
        <field name="model">library.publisher</field>
        <field name="arch" type="xml">
            <tree string="Penerbit">
                <field name="name"/>
                <field name="address"/>
                <field name="phone"/>
                <field name="email"/>
            </tree>
        </field>
    </record>
</odoo>